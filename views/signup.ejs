    <script type="text/javascript">

        var nameoutput = document.getElementById('nameoutput');
        var emailoutput = document.getElementById('emailoutput');
        var usernameoutput = document.getElementById('usernameoutput');
        var phoneoutput = document.getElementById('phoneoutput');
        var genderoutput = document.getElementById('genderoutput');
        var passwordoutput = document.getElementById('passwordoutput');
        var cpasswordoutput = document.getElementById('cpasswordoutput');

        var fullname = document.regform.fullname;
        var email = document.regform.email;
        var username = document.regform.username;
        var telephone_number = document.regform.telephone_number;
        var gender = document.regform.gender;
        var password = document.regform.password;
        var confirm_password = document.regform.confirm_password;
        var error = 0;

        function checkName() {
            if (fullname.value.length < 5) {
                nameoutput.innerHTML = "Fullname must be more than 6 Characters";
                fullname.style.backgroundColor = "red";
                return false;
                error = 1;
            } else if (fullname.value.length > 5) {
                fullname.style.backgroundColor = "lightgreen";
                nameoutput.style.display = "none";
                return true;
                error = 0;
            }
        }

        function checkMail() {
            if (email.value.length < 5) {
                emailoutput.innerHTML = "Please Enter a Valid Email";
                email.style.backgroundColor = "red";
                return false;
                var error = 1;
            } else {
                email.style.backgroundColor = "lightgreen";
                emailoutput.style.display = "none";
                return true;
                var error = 0;
            }
        }

        function checkUsername() {
            if (username.value.length == '') {
                usernameoutput.innerHTML = "This Field is Required!";
                username.style.backgroundColor = "red";
                return false;
                var error = 1;
            } else {
                username.style.backgroundColor = "lightgreen";
                usernameoutput.style.display = "none";
                return true;
                var error = 0;
            }
        }

        function checkPassword() {
            if (password.value.length == '') {
                passwordoutput.innerHTML = "Password Can not be Empty";
                password.style.backgroundColor = "red";
                return false;
                var error = 1;
            } else {
                password.style.backgroundColor = "lightgreen";
                passwordoutput.style.display = "none";
                return true;
                var error = 0;
            }
        }

        function checkConfirmPassword() {
            if (confirm_password.value.length == '') {
                cpasswordoutput.innerHTML = "This Field is Required!";
                confirm_password.style.backgroundColor = "red";
                return false;
                var error = 1;
            } else if (password.value !== confirm_password.value) {
                password.value.focus();
                passwordoutput.innerHTML = "Passwords Do Not Match";
                password.stye.backgroundColor = "red";
                confirm_password.style.backgroundColor = "red";
                return false;
                var error = 1;
            } else {
                password.style.backgroundColor = "lightgreen";
                confirm_password.style.backgroundColor = "lightgreen";
                return true;
                var error = 0;
            }
        }

        function submitForm() {
            if (error = 1) {
                return false;
            } else {
                return true;
            }
        }



    </script>

